;;; -*-Emacs-Lisp-*-

;; BITMAP-ELS: list of bitmap-mule modules to install

(setq bitmap-modules
      (if (featurep 'mule)
	  (if (featurep 'xemacs)
	      nil
	    (cons (if (and (fboundp 'set-buffer-multibyte)
			   (subrp (symbol-function 'set-buffer-multibyte)))
		      'bitmap-ci
		    'bitmap-bi)
		  '(bitmap x-face-mule smiley-mule po)
		  ))))

;;; BITMAP-ELS ends here
